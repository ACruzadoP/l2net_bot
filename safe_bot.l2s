INCLUDE "STDLIB\player.l2c"

DEFINE_GLOBAL ARRAYLIST PL_AL
DEFINE_GLOBAL ARRAYLIST PT_AL
DEFINE_GLOBAL INT X = 1
DEFINE_GLOBAL INT Y = 10

DO

GET_PLAYERS PL_AL
GET_PARTY PT_AL

IF PL_AL.COUNT != PT_AL.COUNT
FORCE_LOG
X = 10
ENDIF

PL_AL.CLEAR
PT_AL.CLEAR

SLEEP 1000

LOOP X == Y

END_SCRIPT